SELECT*FROM EMPLOYEEW;
SELECT DEPT FROM EMPLOYEEW;
SELECT DEPT,COUNT(DEPT)"TOTAL NUMBER OF DEPARTMENTS" FROM EMPLOYEEW GROUP BY DEPT;
SELECT DEPT,COUNT(EMPID)"TOTAL NUMBER OF EMPLOYEES"FROM EMPLOYEEW GROUP BY DEPT HAVING DEPT='MCA';
SELECT DEPT,MAX(SALARY) FROM EMPLOYEEW GROUP BY DEPT;
SELECT DEPT,AVG(SALARY) FROM EMPLOYEEW GROUP BY DEPT;
SELECT COUNT(EMPID) FROM EMPLOYEEW;
SELECT COUNT(EMPID) FROM EMPLOYEEW GROUP BY DEPT;
SELECT MAX(DOJ) FROM EMPLOYEEW;
SELECT MIN(DOJ) FROM EMPLOYEEW;
