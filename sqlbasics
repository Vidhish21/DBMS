CREATE TABLE Student1 (
    ID NUMBER(5),
    Name VARCHAR(25),
    Age NUMBER(3),
    Admission_date DATE,
    AD_fee NUMBER(10)
);
DESCRIBE Student1;
INSERT INTO Student1 VALUES(1, 'VIDHI', 23, '15-JAN-2022',20000);
INSERT INTO Student1 VALUES(2, 'PARIDHI', 26, NULL, 25000);
INSERT INTO Student1 VALUES(3, 'SMITA', 24,'20-MAY-2021', 15000);
SELECT * FROM Student1 WHERE AD_fee<=20000;
SELECT * FROM Student1 WHERE Admission_date is null;
UPDATE Student1 SET AGE=25 WHERE NAME='PARIDHI';
DELETE FROM Student1 WHERE AGE=25;
SELECT * FROM Student1;
CREATE TABLE EMPLOYEE2 (
    EMPID NUMBER(4),
    FNAME VARCHAR(25),
    LNAME VARCHAR(25),
    SAL DECIMAL(7,2)
);
INSERT INTO EMPLOYEE2 VALUES(1, 'AMAN', 'SHARMA', 20000);
INSERT INTO EMPLOYEE2 VALUES(2, 'ROHAN', 'TYAGI', 15000);
INSERT INTO EMPLOYEE2 VALUES(3, 'ROHIT','SHARMA', 25000);

SELECT * FROM EMPLOYEE1;
SELECT EMPID, NAME, SAL, SAL*12 FROM EMPLOYEE;
SELECT * FROM EMPLOYEE WHERE EMPID <> 1;
SELECT LNAME||FNAME AS "NEWNAME" FROM EMPLOYEE1;
CREATE TABLE DETAILS (
    NAME VARCHAR(20),
    CITY VARCHAR(20)
);
INSERT INTO DETAILS VALUES('AMIT','DELHI');
INSERT INTO DETAILS VALUES('AMIT','GHAZIABAD');
INSERT INTO DETAILS VALUES('SHUBHAM','GHAZIABAD');
INSERT INTO DETAILS VALUES('RACHNA','ALLAHABAD');
SELECT * FROM DETAILS;
SELECT NAME || ' is from ' || CITY FROM DETAILS;
SELECT NAME || ' is from ' || CITY AS "EMPLOYEE DETAILS" FROM DETAILS;
SELECT DISTINCT NAME FROM DETAILS;
CREATE TABLE EMP (
    EMPID NUMBER(3),
    NAME VARCHAR(20),
    SALARY NUMBER(7,2)
    );
INSERT INTO EMP VALUES(1, 'AMAN', 15000);
INSERT INTO EMP VALUES(2, 'VARUN', 20000);
INSERT INTO EMP VALUES(3, 'RAKESH', 17000);
SELECT * FROM EMP;
DELETE FROM EMP WHERE EMPID=1;
SELECT NAME, SALARY, SALARY*12 FROM EMP;
SELECT NAME, SALARY FROM EMP WHERE SALARY BETWEEN 16000 AND 20000;
SELECT * FROM EMP WHERE NAME LIKE'A%';
SELECT * FROM EMP ORDER BY NAME DESC;
SELECT * FROM EMPLOYEE WHERE EMPID=1 OR EMPID=3;
SELECT LOWER('AMAN') FROM EMP;
SELECT INITCAP(NAME) FROM EMP;
SELECT CONCAT(EMPID ,NAME) FROM EMP;
SELECT LENGTH('AMAN') FROM EMP;
SELECT ASCII('AMAN') FROM EMP;
SELECT CHR(65) FROM EMP;
