CREATE TABLE student(
    id INT PRIMARY KEY,
    name VARCHAR(50),
    age INT NOT NULL
);
INSERT INTO student VALUES(1, 'VIDHI', 23);
INSERT INTO student VALUES(2, 'SAATVIK', 19);
SELECT * FROM student;
CREATE TABLE ST2(
    id INT UNIQUE,
    name VARCHAR(25)
);
INSERT INTO ST2 VALUES(1,'VIDHI');
INSERT INTO ST2 VALUES(2,'LARA');
SELECT * FROM ST2;
/* primary key constaint*/
